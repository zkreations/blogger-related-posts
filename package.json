    "url": "https://github.com/zkreations/blogger-related-posts/issues"
  },
  "homepage": "https://github.com/zkreations/blogger-related-posts#readme",
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "browserslist": "^4.24.4",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "rollup": "^4.40.0",
    "terser": "^5.39.0"
